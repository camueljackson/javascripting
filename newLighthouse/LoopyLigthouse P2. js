function loopyLighthouse(range, multiples, words) {
  for (var i = range[0]; i <= range[1]; i++) {

    } if( i % multiples[0] === 0 ) {
      console.log(words[1]);

    } else if( i % multiples[1] === 0 ) {
      console.log(words[1]);

    } else {
      console.log(i);
    }
  }
console.log(loopyLighthouse([15, 90], [2, 5], ["Batty", "Beacon"]));